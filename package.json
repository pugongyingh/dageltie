{
  "name": "dageltie",
  "description": "Dageltie static website hosted on Netlify",
  "version": "1.0.0",
  "repository": "https://github.com/richard-flosi/dageltie",
  "author": "Richard Flosi <richard.flosi@gmail.com>",
  "dependencies": {},
  "devDependencies": {
    "concurrently": "4.0.1",
    "dotenv": "6.0.0",
    "netlify-lambda": "0.4.0",
    "serve": "10.0.2"
  },
  "keywords": [
    "dageltie",
    "netlify"
  ],
  "scripts": {
    "start": "concurrently 'npm run start:functions' 'npm run start:public'",
    "start:functions": "netlify-lambda serve functions",
    "start:public": "serve --no-clipboard --config serve.json"
  }
}
